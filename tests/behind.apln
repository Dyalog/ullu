:Namespace behind
    Assert←#.unittest.Assert

      model←{
          ⍺←⍵
          (⍺⍺ ⍺)⍵⍵ ⍵
      }

    ∇ r←testDesc
      r←'for ',case,' & ⎕CT ⎕DCT ⎕FR:',⍕⎕CT ⎕DCT ⎕FR
    ∇

    ∇ {r}←test_behind
      r←⍬
      ⍝ making a palindrome function with behind
      ⍝ TODO: change this so the operator can change functions dynamically
      f←⌽ ⋄ g←≡
      RunVariations←(f model g)#.testfns._RunVariationsWithModel_(f⍛g)
        ⍝ Ints←#.random.Ints
        ⍝ Chars←#.random.Chars
      :For ct :In 0 1 10 0.1
          (⎕CT ⎕DCT)←ct×#.utils.(ct_default dct_default)
          :For fr :In 1 2
              ⎕FR←fr⊃#.utils.(fr_dbl fr_decf)
              ⎕IO←1
     
              case←'setup'
              quadparams←⎕CT ⎕DCT ⎕FR ⎕IO ⎕DIV
              desc←testDesc
              r,←'T1'desc Assert 0≡⌽⍛≡'Dyalog'
              r,←'T2'desc Assert 1≡⌽⍛≡'racecar'
              r,←'T3'desc quadparams RunVariations'racecar'
          :EndFor
      :EndFor
    ∇
:EndNamespace
